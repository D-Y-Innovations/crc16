{
  "name": "node-crc16",
  "version": "1.0.0-beta",
  "description": "A native node addon to calcalate buffer CRC16 values adopted by Modbus-RTU agreement.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --reporter spec",
    "build:node": "npm run clean && node-gyp rebuild",
    "build:xcode-project": "node-gyp configure -- -f xcode",
    "test:cc": "clang++ -Wc++11-extensions -std=c++11 -o ./test/crc16_test.out ./test/crc16_test.cc && ./test/crc16_test.out",
    "test:node": "npm test",
    "clean": "rm -fr build & rm -fr **/*.dSYM & rm -f **/*.out"
  },
  "engines": {
    "node": ">=4.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imnemo/crc16.git"
  },
  "keywords": [
    "crc16",
    "ModbusRTU",
    "native",
    "addon"
  ],
  "author": "imnemo <oncoding1988@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imnemo/crc16/issues"
  },
  "homepage": "https://github.com/imnemo/crc16#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "4.1.0",
    "chance": "1.0.10",
    "mocha": "3.4.2",
    "nyc": "11.1.0"
  }
}
